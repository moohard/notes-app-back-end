{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style" : "off",
        "quotes": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-tabs": "off"
    }
}
